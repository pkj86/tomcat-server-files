<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="AUserDAO">
	<resultMap type="AUSER" id="AUSERMAP">
		<result column="no" property="no" />
		<result column="id" property="uID" />
		<result column="name" property="uName" />
		<result column="pass" property="uPass" />
	</resultMap>
	
    <select id="getidAll" resultMap="AUSERMAP">
    	select no, id, name, pass 
    	from t86_ajax_users
    </select>    
    <select id="getidOne" resultMap="AUSERMAP" parameterType="int">
    	select id, name, pass
    	from t86_ajax_users
    	where no = #{no}
    </select>
</mapper>